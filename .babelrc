{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-strict-mode",
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties",
    [
      "file-loader",
      {
        "name": "[hash].[ext]",
        "extensions": [
          "png",
          "jpg",
          "jpeg",
          "gif",
          "svg"
        ],
        "publicPath": "/images",
        "outputPath": "./static/images",
        "context": "",
        "limit": 0
      }
    ]
  ]
}